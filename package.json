{
  "name": "normalize.css",
  "version": "7.0.0",
  "description": "A modern alternative to CSS resets",
  "main": "normalize.css",
  "style": "normalize.css",
  "files": [
    "LICENSE.md",
    "normalize.css"
  ],
  "devDependencies": {
    "stylelint": "^7.9.0",
    "stylelint-config-standard": "^16.0.0"
  },
  "scripts": {
    "test": "stylelint normalize.css"
  },
  "repository": "dynmap/normalize.css",
  "license": "MIT",
  "bugs": "https://github.com/dynmap/normalize.css/issues",
  "stylelint": {
    "extends": "stylelint-config-standard",
    "rules": {
      "font-family-no-duplicate-names": [
        true,
        {
          "ignoreFontFamilyNames": ["monospace"]
        }
      ]
    }
  }
}
